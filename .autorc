{
  "plugins": [
    "git-tag",
    "released",
    [
      "omit-release-notes",
      {
        "labels": "release"
      }
    ],
    [
      "upload-assets",
      [
        "./artifacts/**/*.zip",
        "./artifacts/**/*.tar.gz"
      ]
    ]
  ],
  "onlyPublishWithReleaseLabel": false,
  "labels": [
    {
      "name": "skip-release",
      "releaseType": "skip",
      "default": true
    }
  ]
}
